per_frame_3=// -------------------------------- Beat Detective v0.07 ----------------------------------
per_frame_4=sure=if(equal(sure,0),.6,sure);
per_frame_5=interval=if(equal(interval,0),40,interval);
per_frame_6=lastbeat=if(equal(lastbeat,0),frame-FPS,lastbeat);
