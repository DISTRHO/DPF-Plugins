per_frame_11=sure=max(.5,sure);
per_frame_12=cheat=if(above(frame,lastbeat+interval+ int(FPS/10))*above(sure,.91),1,cheat);
per_frame_13=beat=if(cheat,1,beat);
per_frame_14=sure=if(cheat,.95*sure,sure);
per_frame_15=maxdbass=max(maxdbass*.999,dbass);
per_frame_16=maxdbass=max(.012,maxdbass);
per_frame_17=maxdbass=min(.02,maxdbass);
per_frame_18=interval=if(beat, frame-lastbeat,interval);
per_frame_19=lastbeat=if(beat,frame-cheat*int(FPS/10),lastbeat);
per_frame_20=cheat=0;
per_frame_21=pbass=bass;
