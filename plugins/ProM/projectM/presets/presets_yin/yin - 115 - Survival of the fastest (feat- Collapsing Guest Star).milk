per_frame_87=wave_r = if( equal( iIndex,1 ), 1.0, wave_r );
per_frame_88=wave_g = if( equal( iIndex,1 ), 0.1, wave_g );
per_frame_89=wave_b = if( equal( iIndex,1 ), 0.1, wave_b );
