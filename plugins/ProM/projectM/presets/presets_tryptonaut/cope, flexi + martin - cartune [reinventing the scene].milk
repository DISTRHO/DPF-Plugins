per_frame_19=
per_frame_20=mx = max(max(bb,mm),tt);
per_frame_21=mn = min(min(bb,mm),tt);
per_frame_22=
per_frame_23=t1 = (bb-mn)/(mx-mn);
per_frame_24=t2 = (mm-mn)/(mx-mn);
per_frame_25=t3 = (tt-mn)/(mx-mn);
per_frame_26=
per_frame_27=v = 0.1/fps;
per_frame_28=bm = bm + (t1-t2)*v;
per_frame_29=mt = mt + (t2-t3)*v;
