per_frame_43=ib_a=.5+.5*sin(q2*2.14+q3);
per_frame_44=mv_a=mv_a+mv_a*sin(q3*2.14+q2);
per_frame_45=ob_size=.1+.1*sin(q3*3+q1);
per_frame_46=ib_size=ib_size*.5+ib_size*.25*sin(q1*3+q3);
per_frame_47=wave_mode=q6+above(q4,0)+above(q5,0);
per_frame_48=wave_mystery=sin(q3*1.14+q1*1.14+q2);
per_frame_49=mv_l=(q6*q7)*q2;
per_frame_50=wave_x=wave_x+.1*q7*q4;
per_frame_51=wave_y=wave_y+.1*q6*q5;
