warp_15=`  n += saturate(tex2D(sampler_pw_main, uv+d*float2(-1,-1)).x*999);
warp_16=`  
warp_17=`  if (n < 1.5)
warp_18=`    ret = 0;
warp_19=`  if (n > 3.5)
warp_20=`    ret = 0;
warp_21=`  if (ret.x < 0.1 && abs(n-3) < 0.5)
warp_22=`    ret = 1;
warp_23=`
warp_24=`  // darken (decay) over time
warp_25=`  ret = (ret - 0.016)*0.985;
warp_26=`}
